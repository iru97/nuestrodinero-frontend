{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "node copy && firebase emulators:start --only functions,hosting",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "node copy && firebase deploy --only functions,hosting",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "firebase-tools": "^8.4.1",
    "fs-extra": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
